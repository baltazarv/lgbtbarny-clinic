{
  "name": "lgbtbarny-clinic",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "npm": "6.9.0",
    "node": "10.16.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "start-client": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run start:dev\" \"npm run start-client\"",
    "dev:testprod": "concurrently --kill-others-on-fail \"npm run start:prod\" \"npm run start-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Baltazar Villegas",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "html-to-text": "^5.1.1",
    "http-auth": "^3.2.4",
    "nodemailer": "^6.4.16",
    "nunjucks": "^3.2.0",
    "sendgrid": "^5.2.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.0"
  }
}
